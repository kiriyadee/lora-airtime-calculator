{
  "name": "airtime-calculator",
  "description": "Airtime calculator for LoRaWAN",
  "version": "1.3.0",
  "author": "Arjan van Bentem",
  "private": true,
  "homepage": "https://avbentem.github.io/airtime-calculator",
  "repository": "https://github.com/avbentem/airtime-calculator",
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "npm run lint",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint:es": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:style": "stylelint 'src/**/*'",
    "lint:pretty": "prettylint '**/*.{html,json,md,yaml}'",
    "lint": "npm run lint:es || npm run lint:style || npm run lint:pretty",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint",
    "src/**/*": "stylelint",
    "**/*.{html,json,md,yaml}": "prettylint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/dom": "^7.31.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/enzyme": "^3.10.18",
    "@types/enzyme-adapter-react-16": "^1.0.9",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.17.7",
    "@types/node": "^14.18.63",
    "@types/react": "^16.14.60",
    "@types/react-dom": "^16.9.24",
    "@types/react-plotly.js": "^2.6.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^16.9.12",
    "axios": "^1.7.9",
    "bootstrap": "^5.3.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.8",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest-dom": "^3.9.4",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-testing-library": "^3.10.2",
    "gh-pages": "^6.2.0",
    "husky": "^4.3.8",
    "jest-enzyme": "^4.2.0",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.21",
    "plotly.js-basic-dist": "^1.58.5",
    "prettier": "^2.8.8",
    "prettylint": "^2.0.0",
    "react": "^16.14.0",
    "react-bootstrap": "^1.6.8",
    "react-dom": "^16.14.0",
    "react-github-corner": "^2.5.0",
    "react-icons": "^3.11.0",
    "react-markdown": "^9.0.1",
    "react-plotly.js": "^2.6.0",
    "react-router-dom": "^5.3.4",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^16.14.0",
    "stylelint": "^16.11.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^6.10.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "sass": "^1.82.0"
  }
}
